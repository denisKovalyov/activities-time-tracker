{
  "name": "activities-time-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rm -r .next node_modules",
    "db:reset": "node -r dotenv/config ./scripts/db-reset.js",
    "db:migrate": "node -r dotenv/config ./scripts/db-migrate.js",
    "db:setup": "node -r dotenv/config ./scripts/db-setup.js && npm run db:migrate",
    "db:restart": "npm run db:reset && npm run db:setup && npm run db:migrate",
    "lint": "next lint",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "test": "vitest run",
    "bundle:analyze": "ANALYZE=true npm run build",
    "dev": "next dev -p 4000",
    "start": "next start"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@phosphor-icons/react": "^2.1.5-alpha.3",
    "@raddix/use-media-query": "^0.1.3",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@types/react-virtualized": "^9.21.30",
    "@vercel/postgres": "^0.10.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "crypto": "^1.0.1",
    "framer-motion": "^11.11.4",
    "i": "^0.3.7",
    "next": "^14.2.13",
    "next-auth": "^5.0.0-beta.19",
    "next-themes": "^0.3.0",
    "npm": "^10.8.3",
    "react": "^18",
    "react-colorful": "^5.6.1",
    "react-dom": "^18",
    "react-hook-form": "^7.51.2",
    "react-swipeable": "^7.0.1",
    "react-timer-hook": "^3.0.7",
    "resend": "^3.2.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.0",
    "vaul": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.13",
    "@tailwindcss/typography": "^0.5.12",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.12.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vercel/style-guide": "^6.0.0",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^2.0.5"
  }
}
